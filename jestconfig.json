{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
      "src/**/*.ts"
  ],
  "testRegex": "(/src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "modulePathIgnorePatterns": ["/lib/"]
}